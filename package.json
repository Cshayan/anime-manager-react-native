{
  "name": "animemanager",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "devDebug": "react-native run-android --variant=devDebug",
    "prodDebug": "react-native run-android --variant=prodDebug",
    "devRelease": "npx jetify && cd android && gradlew clean && gradlew assembleDevRelease && cd ..",
    "prodRelease": "npx jetify && cd android && gradlew clean && gradlew assembleProdRelease && cd ..",
    "prodBundle": "npx jetify && cd android && gradlew clean && gradlew bundleProdRelease && cd ..",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "precommit": "eslint",
    "prepush": "npm run lint"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.15.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "5.11.9",
    "@react-navigation/drawer": "5.12.5",
    "@react-navigation/native": "5.9.4",
    "@react-navigation/stack": "5.14.4",
    "axios": "0.21.1",
    "formik": "2.2.6",
    "loadash": "^1.0.0",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-config": "^1.4.2",
    "react-native-gesture-handler": "1.10.3",
    "react-native-keyboard-aware-scroll-view": "0.9.3",
    "react-native-reanimated": "2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "3.1.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-swiper-flatlist": "3.0.14",
    "react-native-vector-icons": "8.1.0",
    "react-query": "3.13.6",
    "react-redux": "7.2.3",
    "redux-thunk": "2.3.0",
    "rn-placeholder": "3.0.3",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.24.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "3.1.0",
    "jest": "^26.6.3",
    "lint-staged": "10.0.8",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "npm run precommit"
  }
}
